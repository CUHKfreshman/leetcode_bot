{
  "name": "leetcode_bot",
  "version": "1.0.0",
  "description": "Bot for everyday Leetcode problems",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js dotenv_config_path=.env.production",
    "copy-assets": "cp -R src/data dist/",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-assets"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "leetcode-query": "^1.2.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
